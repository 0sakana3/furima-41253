    <% if user_signed_in? && @item.liked_by?(current_user)%>
      <%=link_to "ブックマーク済", item_likes_path(@item.id),  data: { turbo_method: :delete }, class: "item_likes" %>
    <% else %>
      <%=link_to "ブックマーク", item_likes_path(@item.id), data: { turbo_method: :post }, class: "item_likes" %>
    <% end %>