   <% if user_signed_in? && @item.liked_by?(current_user)%>
      <%=link_to "いいね済", item_likes_path(@item.id),  data: { turbo_method: :delete }, class: "item_likes" %>
        <%= item.likes.count %>
    <% else %>
      <%=link_to "いいね", item_likes_path(@item.id), data: { turbo_method: :post }, class: "item_likes" %>
        <%= item.likes.count %>
    <% end %>